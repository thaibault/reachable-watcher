{
  "name": "reachable-watcher",
  "version": "1.0.118",
  "license": "CC-BY-3.0",
  "description": "Check status codes of web servers and sends notification e-mails",
  "homepage": "https://torben.website/reachable-watcher",
  "documentationWebsite": {
    "name": "reachable-watcher",
    "trackingCode": "UA-40192634-15"
